# loop from 1 to 100

srv gn1 di:1    # index
srv gn2 di:1    # increment
srv gn3 di:1000 # limit
srv gn4 di:32   # space 

>1to16
    put gn1 sx:2 # print index
    put gn4 sx:3 # print space 

    add gn1 gn2  # increment

    ilq gn1 gn3  # is gn1 less or equal to gn3
    pbr lqt      # if is or equal to -> jump
:1to16

end

